    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="27" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.kaftand.entomologydatacollect.MainActivity"
            android:configChanges="locale|orientation|keyboardHidden" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.kaftand.entomologydatacollect.HumanLandingCatch"
            android:label="HumanLandingCatch"
            android:launchMode="singleInstance"
            android:screenOrientation="landscape"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.kaftand.entomologydatacollect.HumanLandingCatchIntro"
            android:label="@string/title_activity_human_landing_catch_intro"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>