<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HumanLandingCatch.HumanLandingCatch">




    <LinearLayout
        android:id="@+id/TableHeaderContainer"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="6pt"
            android:text="serial"
            android:id="@+id/serial_label"/>


        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:id="@+id/MainTableContainer">
                <Button
                android:id="@+id/collectDataButton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:onClick="collectGoToNextPage"
                android:text="@string/complete_form"
                tools:layout_editor_absoluteX="25dp"
                tools:layout_editor_absoluteY="511dp" />
            </LinearLayout>
        </ScrollView>
    </LinearLayout>



</android.support.constraint.ConstraintLayout>